thetas = zeros(21, 100);
thetas(:, end) = ones(21, 1);

for i = 2:49
    thetas(:, i) = randn(21, 1);
end

for i = 1:50
    
    i
    
    [f, dist, iter_num] = admm_trust(q0, alpha0, thetas(:, i), mu, delta0, delta_max, lambda, x, y);

    m = mean(dist, 2);

    plot(1:iter_num, log(m));
    hold on;
    
    

end

