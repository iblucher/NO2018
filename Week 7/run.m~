q0 = zeros(42, 1);
alpha0 = zeros(42, 1);
mu = 1;
delta0 = 1;
delta_max = 10;
lambda = 1;
[x, y] = gen_data(10, 20);

[f, dist, iter_num] = admm_trust(q0, alpha0, zeros(21, 1), mu, delta0, delta_max, lambda, x, y);
m = mean(dist, 2);
plot(1:iter_num, log(m));
hold on;
    
