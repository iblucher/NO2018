function [p, q, alpha] = subproblem(q0, alpha0, mu0, rho, theta, x, y)
    
    % initialize variables
    k = 0;
    
    while diff > tol && k < max_iter
        [f, g, B] = compute_function(theta, x, y);
        
        grad = zeros(%dim of p);
        for i = 1:%dimension of p
            grad(i) = g' + B * p(i) + rho * (p(i) - q(i)) - alpha(i);
        end
        
        hess = zeros(%dim of p, %dim of p);
        for i = 1:%dimension of p
            hess(i, i) = B + rho;
        end
        
        % compute Newton step
        
    end
end